    <Style x:Key="RadioButton_xmxx" TargetType="RadioButton">
        <Setter Property="Cursor" Value="Hand" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Margin" Value="20,3,20,3"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="BorderThickness" Value="0,0,0,2"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Foreground" Value="{StaticResource Fg_Text_Dark}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RadioButton">
                    <Border Padding="10,2"  Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid Width="24" Height="24">
                                <Polyline Stroke="{TemplateBinding Foreground}" StrokeThickness="1" StrokeLineJoin="Round" Points="4,4 12,4 12,8 20,8 20,20 4,20 4,4" />
                                <Polyline Stroke="{TemplateBinding Foreground}" StrokeThickness="1" StrokeLineJoin="Round" Points="6,12 10,12" />
                                <Polyline Stroke="{TemplateBinding Foreground}" StrokeThickness="1" StrokeLineJoin="Round" Points="12,12 18,12" />
                                <Polyline Stroke="{TemplateBinding Foreground}" StrokeThickness="1" StrokeLineJoin="Round" Points="6,16 10,16" />
                                <Polyline Stroke="{TemplateBinding Foreground}" StrokeThickness="1" StrokeLineJoin="Round" Points="12,16 18,16" />
                            </Grid>
                            <Label Grid.Column="1" FontSize="16" Foreground="{TemplateBinding Foreground}" Content="{TemplateBinding Content}"/>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="true">
                            <Setter Property="Foreground" Value="{StaticResource Fg_Text_Checked}"/>
                            <Setter Property="BorderBrush" Value="{StaticResource Bd_Checked}"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter Property="BorderBrush" Value="{StaticResource Bd_Checked}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

